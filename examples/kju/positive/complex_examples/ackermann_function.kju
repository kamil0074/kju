/* Let's compute the Ackermann function */

fun a(m : Int, n : Int) : Int
{
    if (m == 0)
    then
    {
        return n + 1;
    }
    else
    {
        if (n == 0)
        then
        {
            return a(m - 1, 1);
        }
        else
        {
            return a(m - 1, a(m, n - 1));
        };
    };
}

fun kju() : Unit
{
    a(3, 3);
}
